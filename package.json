{
    "name": "angularjs-seed",
    "version": "1.0.3",
    "description": "A Simple Skeleton for AngularJS application",
    "scripts": {
        "update-deps": "npm update",
        "start": "grunt connect",
        "develop": "grunt develop",
        "test": "karma start",
        "test:single-run": "karma start --browsers Firefox --single-run",
        "preupdate-webdriver": "npm install",
        "update-webdriver": "webdriver-manager update",
        "preprotractor": "npm run update-webdriver",
        "protractor": "protractor protractor.conf.js",
        "precommit": "npm run test:single-run",
        "cz": "git-cz"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Sibiraj-S/angularjs-seed.git"
    },
    "keywords": [
        "angularjs-seed",
        "angular-application",
        "angular-app-with-authentication"
    ],
    "author": "Sibiraj",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Sibiraj-S/angularjs-seed/issues"
    },
    "homepage": "https://github.com/Sibiraj-S/angularjs-seed",
    "dependencies": {
        "@uirouter/angularjs": "^1.0.15",
        "angular": "^1.6.9",
        "angular-loading-bar": "^0.9.0",
        "angular-resource": "^1.6.9",
        "angular-sanitize": "^1.6.9"
    },
    "devDependencies": {
        "angular-mocks": "^1.6.9",
        "commitizen": "^2.9.6",
        "cz-conventional-changelog": "^2.1.0",
        "grunt": "^1.0.2",
        "grunt-contrib-connect": "^1.0.2",
        "grunt-contrib-sass": "^1.0.0",
        "grunt-contrib-watch": "^1.0.0",
        "husky": "^0.14.3",
        "jasmine-core": "^3.1.0",
        "karma": "^2.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-firefox-launcher": "^1.1.0",
        "karma-jasmine": "^1.1.1",
        "karma-jasmine-html-reporter": "^1.0.0",
        "karma-mocha-reporter": "^2.2.5",
        "load-grunt-tasks": "^3.5.2",
        "protractor": "^5.3.0"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    }
}
